{
  "name": "valence",
  "description": "JavaScript Valence SDK",
  "version": "1.0.1",
  "homepage": "http://docs.valence.desire2learn.com",
  "author": {
    "name": "Desire2Learn"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Desire2Learn-Valence/valence-sdk-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/Desire2Learn-Valence/valence-sdk-javascript/issues"
  },
  "licenses": [
    {
      "type": "Apache",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "main": "valence.js",
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "test": "./node_modules/.bin/istanbul test ./node_modules/.bin/_mocha -- -R spec spec",
    "report-cov": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build": "./node_modules/.bin/browserify -g uglifyify ./lib/valence.js > browser.js"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "mocha": "^1.18.2",
    "istanbul": "^0.2.7",
    "coveralls": "^2.10.0",
    "sinon": "~1.9.1",
    "browserify": "~3.39.0",
    "uglifyify": "~2.1.1"
  },
  "testling": {
    "harness": "mocha",
    "files": "spec/*.js",
    "browsers": [
      "ie/9..latest",
      "chrome/latest", "chrome/canary",
      "firefox/latest", "firefox/nightly",
      "safari/5.1..latest"
    ]
  }
}
